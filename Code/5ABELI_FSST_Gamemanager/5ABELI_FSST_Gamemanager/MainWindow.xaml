<Window x:Class="_5ABELI_FSST_Gamemanager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_5ABELI_FSST_Gamemanager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_add" Content="Add" HorizontalAlignment="Left" Height="52" Margin="48,238,0,0" VerticalAlignment="Top" Width="133"/>
        <Button x:Name="btn_delete" Content="Delete" HorizontalAlignment="Left" Height="53" Margin="48,295,0,0" VerticalAlignment="Top" Width="133"/>
        <Button x:Name="btn_new_list" Content="New" HorizontalAlignment="Left" Height="53" Margin="48,106,0,0" VerticalAlignment="Top" Width="133"/>
        <Button x:Name="btn_open_list" Content="Open" HorizontalAlignment="Left" Height="53" Margin="48,164,0,0" VerticalAlignment="Top" Width="133"/>
        <ListView HorizontalAlignment="Left" Height="355" Margin="203,0,0,0" VerticalAlignment="Center" Width="574" d:ItemsSource="{d:SampleData ItemCount=5}">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
